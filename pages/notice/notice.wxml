<!--pages/notice/notice.wxml-->
<view class="page">
  <view class="noticeHeader">
    <text>公告板</text>
  </view>
  <view class="notice" bindtap="editAnnouncement">
  {{notice.content}}
  <text class="lastUpdateInfo">
  由 {{notice.lastModifiedBy}} 发布于 {{notice.lastModifiedDate}}
  </text>
  </view>
</view>
